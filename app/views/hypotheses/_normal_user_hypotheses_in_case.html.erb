<div class="margin2em">
  <% @not_checked_hypotheses[group.id].to_a.each do |not_checked_hypothesis| %>

      <div class="row">
        <div class="col-md-9">

          <%= form_for(@new_checked_hypothesis) do |f| %>
              <%= f.hidden_field :exercise_hypothesis_id, :value => not_checked_hypothesis.id %>
              <%= f.hidden_field :user_id, :value => @user.id %>
              <%= f.submit not_checked_hypothesis.hypothesis.name, :class => 'btn btn-default btn-save' %>
          <% end %>

        </div>

        <div class="col-md-3">

        </div>
      </div>
  <% end %>

</div>

<%# add hypothesis back to case for normal user%>
<div class="margin2em">
  <% @checked_hypotheses[group.id].to_a.each do |checked_hypothesis| %>

      <div class="row">
        <div class="col-md-9">
          <%= button_to checked_hypothesis.hypothesis.name, checked_hypothesis, method: :delete, :class => 'btn btn-default btn-checked' %>

        </div>

        <div class="col-md-3">

        </div>
      </div>
  <% end %>

</div>

<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title"></h3>
  </div>
  <div class="panel-body">
    <% if current_user.has_completed?(subtask) || current_user_is_admin %>
      <%= render 'completed_task_text', { subtask:subtask} %>
    <% else %>
      <%= render 'ask_task_text', { subtask:subtask } %>
    <% end %>
  </div>
</div>
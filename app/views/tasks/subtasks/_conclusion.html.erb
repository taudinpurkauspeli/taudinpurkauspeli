<div class="panel panel-default">
  <div class="panel-heading">
     <h3 class="panel-title"><%= subtask.conclusion.title %></h3>
  </div>
  <div class="panel-body">
    <% if current_user.has_completed?(subtask) || current_user.try(:admin) %>
        <%= render 'tasks/subtasks/completed_conclusion', { subtask:subtask, exercise_show_or_task_show: parent_view+subtask.conclusion.id.to_s} %>
    <% else %> 
        <%= render 'tasks/subtasks/ask_conclusion', { subtask:subtask, exercise_show_or_task_show: parent_view+subtask.conclusion.id.to_s } %>
    <% end %> 
  </div>
</div>
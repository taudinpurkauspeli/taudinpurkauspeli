<% subtask.multichoice.options.each do |option| %>
    <tr>
      <td class="multichoice 
      <% if option.is_correct_answer %>
        correct-answer
      <% else %>
        wrong-answer
      <% end %>
      ">
        <%= option.content %>
      </td>
      <td class="multichoice 
      <% if option.is_correct_answer %>
        correct-answer
      <% else %>
        wrong-answer
      <% end %>
      ">
        <%= option.explanation %>
      </td>
    </tr>
<% end %>
</table>
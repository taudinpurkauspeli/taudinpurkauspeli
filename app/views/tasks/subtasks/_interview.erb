<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title"><%= subtask.interview.title %></h3>
  </div>
  <div class="panel-body">
    <% if current_user.has_completed?(subtask) || current_user_is_admin %>
        <%= render 'tasks/subtasks/completed_interview', { subtask:subtask} %>
    <% else %>
        <%= render 'tasks/subtasks/ask_interview', { subtask:subtask } %>
    <% end %>
  </div>
</div>

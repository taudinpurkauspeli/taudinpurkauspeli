<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title"> <%= subtask.interview.title %></h3>
  </div>

  <div class="panel-body">


       <h3 class="panel-title"> Kysymys</h3>
        <% subtask.interview.questions.each do |question| %>
            <div class="row">

              <div class="col-md-4">
               
                     <%= question.title %>
              </div>
              <div class="col-md-4">
                  <%= question.content %>
              </div>
            </div>
        <br />
        
    <% end %> 

  </div>

  <%= if !current_user_has_completed_task(@task.id) && !current_user_is_admin
    then render 'continue_button'
    else
    end %>
</div>






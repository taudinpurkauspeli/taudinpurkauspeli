<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title">Kysymys: <%= subtask.multichoice.question %></h3>
  </div>

  <div class="panel-body">
    <% if current_user.has_completed?(subtask) %>
      <%= render 'completed_multichoice', { subtask:subtask} %>
    <% else %>
      <%= render 'ask_multichoice', { subtask:subtask } %>
    <% end %>
  </div>
</div>